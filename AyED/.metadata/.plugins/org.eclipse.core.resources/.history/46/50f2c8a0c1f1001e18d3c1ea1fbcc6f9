import practica2.BinaryTree;

public class Util {
	
	
	// genera arbol de longitud 10 desde la raiz
	public BinaryTree<Integer> generarArbolEnteros(){
				
		BinaryTree<Integer> tree = new BinaryTree(getRandomNumber(1, 1000));
		int i = 0;
		int lenght = 10;
		
		while(i <= 10) {
			
			BinaryTree<Integer> br = new BinaryTree(getRandomNumber(1, 1000));
			
			i++;
		}
		
		
	}
	
	public int getRandomNumber(int min, int max) {
	    return (int) ((Math.random() * (max - min)) + min);
	}
	
}
