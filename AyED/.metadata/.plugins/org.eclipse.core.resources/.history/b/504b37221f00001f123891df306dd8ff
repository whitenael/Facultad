package practica3.ejercicio2;

import java.util.LinkedList;
import java.util.List;

import practica3.GeneralTree;

public class Recorridos {

	public List<Integer> numerosImparesMayoresPreOrden(GeneralTree<Integer> tree, Integer n) {
		
		List<Integer> nums = new LinkedList<Integer>();
		
		nums = numerosImparesMayoresRecursivo(tree, n, nums);
		
		return nums;		
	}

	private List<Integer> numerosImparesMayoresRecursivo(GeneralTree<Integer> tree, Integer n, List<Integer> nums) {
		
		if (tree == null)
			return null;
		
		if (tree.getData() > n && tree.getData() %2 != 0)
		{
			nums.add(tree.getData());
		}
		
		return null;		
	}
	
}
